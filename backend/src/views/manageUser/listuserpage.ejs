<html lang="en">

<head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>Manage User</title>
</head>
<style>
   table,
   th,
   td {
      border: 1px solid black;
      border-collapse: collapse;
   }
</style>

<body>
   <h1>Manage user</h1>
   <a href="/ma_user_create" class="btn_create" type="button">create user</a>
   <h6></h6>
   <table style="width: 100%;">
      <tr>
         <th>Username</th>
         <th>First name</th>
         <th>Last name</th>
         <th>Telephone</th>
         <th>Email</th>
      </tr>
      <% for(let i=0; i < listUser.length; i++) { %>
         <tr>
            <td>
               <%=listUser[i].username %>
            </td>
            <td>
               <%=listUser[i].first_name %>
            </td>
            <td>
               <%=listUser[i].last_name %>
            </td>
            <td>
               <%=listUser[i].telephone %>
            </td>
            <td>
               <%=listUser[i].email %>
            </td>
         </tr>
         <% } %>
   </table>

</body>

</html>